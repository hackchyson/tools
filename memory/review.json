{
  "paths": [
    "/Users/hack/test"
  ],
  "excludes": [
    ".git",
    "#",
    "~"
  ],
  "output_dir": "/Users/hack/test",
  "file_format": "%Y-%m-%d-%H.review",
  "stop_line": 365,
  "forgetting": [
    0,
    1,
    2,
    4,
    7,
    15
  ],
  "time_format": "[%Y-%m-%d %a]",
  "time_pattern": "\\[[0-9]{4}-[0-9]{2}-[0-9]{2} (?:Sun|Sat|Mon|Tues|Wed|Thur|Fri)\\]",
  "encoding": "utf8",
  "headline_pattern": "^\\*+ ",
  "end": "[hack-end]"
}